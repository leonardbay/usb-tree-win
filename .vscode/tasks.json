{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "npm: build",
            "type": "shell",
            "command": "npm",
            "args": [
                "run",
                "build"
            ],
            "problemMatcher": "$tsc",
            "group": {
                "kind": "build",
                "isDefault": true
            }
        },
        {
            "label": "npm: start",
            "type": "shell",
            "command": "npm",
            "args": [
                "start"
            ],
            "problemMatcher": [],
            "group": {
                "kind": "test",
                "isDefault": true
            }
        },
        {
            "label": "npm: dev",
            "type": "shell",
            "command": "npm",
            "args": [
                "run",
                "dev"
            ],
            "isBackground": true,
            "problemMatcher": {
                "pattern": {
                    "regexp": "^.*$",
                    "file": 1,
                    "location": 2,
                    "message": 3
                },
                "background": {
                    "activeOnStart": true,
                    "beginsPattern": "^.*Starting.*",
                    "endsPattern": "^.*Ready on.*"
                }
            }
        }
    ]
}